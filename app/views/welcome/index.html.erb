<section id="post" class="post">
    <div class="container">
    <h5 style="color: black; margin-top: 30px;">Derniers publications</h5>
        <% @posts.each_slice(3) do |posts| %>
            <div class="row">
                <% posts.each do |post| %>
                    <div class="col-md-4 col-sm-4">
                            <% if post.pdfs.presence %>
                                <iframe src=<%= post.pdfs.first.link %> width="200" height="200"></iframe>
                            <% end %>
                            <p style="font-size: 20px; margin-bottom: 30px"><%= post.name %></p>
                    </div>
                <% end %>
            </div>
        <% end %>
    </div>
</section>

