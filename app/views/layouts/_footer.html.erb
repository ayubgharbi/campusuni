        <%= link_to "https://www.facebook.com/Tanjawa.Family/", target: "_blank"  do %>
            <%= image_tag "tjf1.jpg", class: "tjf" %>
        <% end %>
        </div>
        <div class="col-md-4 col-sm-4 col-xs-4">
        <ul>
        <br>
        <% Deust.all.order("created_at ASC").each do |deust| %>
            <li>
                <h5><%= link_to deust.name, deust_path(deust), class: "footer-deust" %></h5>
            </li>
        <% end %>
        </ul>
    </div>
    </div>

</div>

